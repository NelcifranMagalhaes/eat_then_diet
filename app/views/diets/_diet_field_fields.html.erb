<div class="row">
  <div class='nested-fields'>

  	<div class="row">
  		<div class="input-field col s12">
          	<%= f.input :schedule,as: :time,class: "timepicker"%>
      </div>
    </div>

  	<div class="row">
  		<div class="input-field col s12">
          	<%= f.input :description,as: :text ,class: 'materialize-textarea'%>

        	</div>
    </div>

  	<div class="row">
  		<div class="input-field col s12">
        <%=f.input :meal,class: "browser-default", collection: [ "Dinner", "Lunch", "Breakfast","Snack"]%>
      </div>
    </div>

  	<div class="row">
  	 <%= link_to_remove_association "remove diet field", f ,class: 'waves-effect waves-light btn'%>
  	</div>
  </div>
</div>

<script type="text/javascript">
  $(document).ready(function() {
    $('select').material_select();

  });
</script>